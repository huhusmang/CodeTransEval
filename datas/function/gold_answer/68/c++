string dayOfProgrammer(int year) {
    if (year == 1918) {
        return "26.09.1918";
    }
    bool leap_year = true;
    if ((year % 4 != 0) || (year > 1918 && year % 100 == 0 && year % 400 != 0)) {
        leap_year = false;
    }
    return leap_year ? "12.09." + to_string(year) : "13.09." + to_string(year);
}