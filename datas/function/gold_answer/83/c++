int gcdAndLcm(int x, int y) {
    int n = x * y;
    int ans = 0;
    for (int i = x; i <= y; ++i) {
        if (n % i == 0 && gcd(i, n / i) == x && lcm(i, n / i) == y) {
            ans++;
        }
    }
    return ans;
}
int gcd(int x, int y) {
    if (x < y) {
        std::swap(x, y);
    }
    while (y != 0) {
        int r = x % y;
        x = y;
        y = r;
    }
    return x;
}
int lcm(int x, int y) {
    return x / gcd(x, y) * y;
}