int findFirstPairExceedingSum(vector<vector<int>>& pairs) {
      int maxSum = 0;
      int dayIndex = 0;
      for (int i = 0; i < pairs.size(); ++i) {
          int a = pairs[i][0];
          int b = pairs[i][1];
          int sum = a + b;
          if (sum > 8 && sum > maxSum) {
              maxSum = sum;
              dayIndex = i + 1;
          }
      }
      return dayIndex;
  }